{
  "name": "graphql-tag-schema",
  "version": "1.0.1",
  "description": "GraphQL Code Generator plugin for generating a .js / .ts file from a given schema",
  "repository": "git@github.com:emolr/graphql-tag-schema.git",
  "license": "MIT",
  "scripts": {
    "build": "tsc -m esnext --outDir dist/esnext && tsc -m commonjs --outDir dist/commonjs",
    "test": "jest --config ./jest.config.js"
  },
  "dependencies": {
    "@graphql-codegen/plugin-helpers": "1.2.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.4",
    "graphql": "14.3.1",
    "graphql-tag": "2.10.1",
    "jest": "24.8.0",
    "jest-junit": "^6.4.0",
    "ts-jest": "24.0.2",
    "tslib": "1.9.3",
    "typescript": "3.5.1"
  },
  "peerDependencies": {
    "graphql": "^0.8.0 || ^0.9.0 || ^0.10.0 || ^0.11.0 || ^0.12.0 || ^0.13.0 || ^14.0.0"
  },
  "sideEffects": false,
  "main": "dist/commonjs/index.js",
  "module": "dist/esnext/index.js",
  "typings": "dist/esnext/index.d.ts",
  "typescript": {
    "definition": "dist/esnext/index.d.ts"
  },
  "jest-junit": {
    "outputDirectory": "./test-results/graphql-tag-schema"
  },
  "publishConfig": {
    "access": "public"
  }
}
